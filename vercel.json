{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|ico|svg|avif|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/impressum",
      "dest": "/public/impressum.html"
    },
    {
      "src": "/datenschutz",
      "dest": "/public/datenschutz.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}

